<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <!-- Creating the observation page for the sample specification sheet-->
        <record id="specification_observation_page" model="survey.page">
            <field name="title">Observation</field>
            <field name="survey_id" ref="its_mrp_product_survey_sample.specification_sheet_sample"/>
        </record>

        <!-- Creating questions for the observation page-->
        <record id="specification_question_observation" model="survey.question">
            <field name="question">OBSERVATION</field>
            <field name="type">free_text</field>
            <field name="page_id" ref="its_mrp_product_survey_sample.specification_observation_page"/>
        </record>
    </data>
</odoo>


